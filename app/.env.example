# PeerJS Server Host (leave empty for default cloud server)
VITE_PEERJS_HOST=

# PeerJS Server Port (leave empty for default)
VITE_PEERJS_PORT=

# PeerJS Server Path (leave empty for default)
VITE_PEERJS_PATH=

# Use secure connection (true/false)
VITE_PEERJS_SECURE=true

# STUN Server Configuration
# STUN server URL for WebRTC peer connections (leave empty for default: stun:stun.l.google.com:19302)
VITE_STUN_SERVER_URL=

# Password Protection
# Maximum number of password retry attempts per participant (default: 5)
VITE_MAX_PASSWORD_RETRIES=5